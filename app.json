{
    "name": "My PMBot",
    "description": "A simple small pm assistant bot",
    "repository": "https://github.com/InnocentLuckyRajput/PMBOT",
    "keywords": [
        "telegraph",
        "file or media",
        "link uploader",
        "telegram bot",
        "pyrogram"
    ],
    "env": {
        "TOKEN": {
            "description": "Your Bot token from @Botfather"
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps "
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        },
        "STRING_SESSION": {
            "description": "Your String_Session",
            "value": ""
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
